#include "main.h"
/**
 * times_table - main func
 * Return: void
 */
void times_table(void)
{
int i, j, res;
for (i = 0; i < 10; i++)
{
for (j = 0; j < 10; j++)
{
res = i * j;
if (j == 0)
{
_putchar(48);
}
else
{
_putchar(',');
if (res < 10)
_putchar(' ');
_putchar(' ');
if (res > 9)
_putchar((res / 10) + 48);
_putchar((res % 10) + 48);
}
}
_putchar('\n');
}
}
